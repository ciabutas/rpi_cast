{
   "manifest_version": 3,
   "name": "RaspberryCast",
   "version": "1.4.1",
   "description": "Share videos to Rpi",
   "background": {
      "scripts": ["js/background.js"],
      "type": "module"
   },
   "action": {
      "default_icon": "20.png",
      "default_title": "RasperryCast remote",
      "default_popup": "popup.html"
   },
   "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
   },
   "options_ui": {
      "page": "options.html",
      "open_in_tab": true
   },
   "permissions": [
      "storage",
      "contextMenus",
      "tabs",
      "notifications"
   ],
   "host_permissions": [
      "http://*/"
   ]
}
